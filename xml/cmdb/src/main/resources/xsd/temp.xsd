      <sequence>
        <element name="plugin">
          <complexType>
            <sequence>
              <element name="type">
                <complexType>
                  <sequence>
                    <element name="item" maxOccurs="unbounded" minOccurs="0">
                      <complexType>
                        <simpleContent>
                          <extension base="string">
                            <attribute type="string" name="mapRegex" use="optional"/>
                            <attribute type="string" name="val" use="optional"/>
                          </extension>
                        </simpleContent>
                      </complexType>
                    </element>
                  </sequence>
                </complexType>
              </element>
            </sequence>
            <attribute type="string" name="id"/>
            <attribute type="string" name="typeColumn"/>
          </complexType>
        </element>
      </sequence>
      <attribute type="string" name="destColumn"/>
      <attribute type="string" name="srcColumn"/>
    </complexType>
  </element>
</schema>